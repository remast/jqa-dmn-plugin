<jqa-plugin:jqassistant-plugin xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.4"
                               name="Camunda DMN">
    <description>Provides a scanner for Camunda DMN (*.dmn) descriptors.</description>
    <model>
        <class>org.jqassistant.contrib.plugin.dmn.model.DmnXmlDescriptor</class>
        <class>org.jqassistant.contrib.plugin.dmn.model.DmnDecisionDescriptor</class>
        <class>org.jqassistant.contrib.plugin.dmn.model.DmnDecisionInput</class>
        <class>org.jqassistant.contrib.plugin.dmn.model.DmnDecisionOutput</class>
    </model>
    <scanner>
        <class>org.jqassistant.contrib.plugin.dmn.scanner.DmnXmlScannerPlugin</class>
    </scanner>
</jqa-plugin:jqassistant-plugin>
